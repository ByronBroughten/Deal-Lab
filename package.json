{
  "name": "node",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "concurrently \"nodemon -r dotenv/config --inspect src/index.ts --ignore client/\" \"npm run client\"",
    "start:dev:debug": "nodemon --inspect src/index.ts",
    "client": "cd src/client && npm run start",
    "start:prod": "ts-node src/index.ts",
    "install": "cd src/client && npm install",
    "build": "cd src/client && npm run build",
    "heroku-postbuild": "cd src/client && npm install --only=dev && npm install && npm run build",
    "test": "jest --watchAll --verbose --coverage"
  },
  "engines": {
    "node": "14.15.5",
    "npm": "6.14.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/busterbyron/Property-Analyzer-Server-Side.git"
  },
