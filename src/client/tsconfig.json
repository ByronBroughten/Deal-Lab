{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": true,
    "jsx": "react-jsx",
    "noEmit": true
  },
  "include": [
    "src",
    "src/App/sharedWithServer/exampleMakers/initDbStoreArrs.ts"
  ],
  "exclude": ["node_modules", ".vscode", "*.json"]
}
